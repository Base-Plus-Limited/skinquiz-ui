{
  "name": "diagnostic-tool-server",
  "version": "1.0.0",
  "description": "Backend for diagnostic tool",
  "scripts": {
    "start": "npm run compile && node server",
    "build": "cd react-ui/ && npm install && npm run build",
    "compile": "tsc --skipLibCheck --esModuleInterop server/index.ts server/server.ts",
    "start-all": "concurrently \"npm run compile && node server\" \"npm run start-frontend\"",
    "start-frontend": "cd react-ui/ && npm run start"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/superagent": "^4.1.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "html-entities": "^1.2.1",
    "mixpanel": "^0.11.0",
    "mongoose": "^5.7.11",
    "superagent": "^5.1.0",
    "typescript": "3.5.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/html-entities": "^1.2.16",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.6.8",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0"
  },
  "engines": {
    "node": "10.12.0",
    "npm": "6.10.2"
  }
}
